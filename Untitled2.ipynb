        "def prime_number(num):\n",
        "  while num<2 and num==2 and num%2 ==0:\n",
        "    return False\n",
        "  for a in range(3,int(num//2),2):\n",
        "    while (num%a==0):\n",
        "      return False\n",
        "  return True"
      ]
    }
  ]
